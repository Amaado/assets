<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.ProyectoSede" table="proyecto_sede">
        <composite-id>
            <key-property name="idProy" column="id_proy" type="int"/>
            <key-property name="idSede" column="id_sede" type="int"/>
        </composite-id>
        <property name="fInicio" column="f_inicio" type="date" not-null="true"/>
        <property name="fFin" column="f_fin" type="date" not-null="false"/>

    </class>
</hibernate-mapping>